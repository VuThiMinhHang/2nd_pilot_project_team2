<% provide(:title, 'Create New Team') %>
<h1>Create group</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@team) do |f| %>
    	<%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Create Group", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>