<% provide(:title, "Create Album") %>
<h1>
  Create album
</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for @image, url: user_album_images_path, method: :post do |f| %>
      <%= render "shared/error_messages", object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.submit "Create image", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>